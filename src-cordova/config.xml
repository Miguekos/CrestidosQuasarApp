<?xml version='1.0' encoding='utf-8'?>
<widget id="org.quasar.prestamos" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Quasar App</name>
    <description>Control y Admin de Pagos</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <icon density="ldpi" src="../app-resources/android/icons/icon-36-ldpi.png" />
        <icon density="mdpi" src="../app-resources/android/icons/icon-48-mdpi.png" />
        <icon density="hdpi" src="../app-resources/android/icons/icon-72-hdpi.png" />
        <icon density="xhdpi" src="../app-resources/android/icons/icon-96-xhdpi.png" />
        <icon density="xxhdpi" src="../app-resources/android/icons/icon-144-xxhdpi.png" />
        <icon density="xxxhdpi" src="../app-resources/android/icons/icon-192-xxxhdpi.png" />
        <splash density="land-hdpi" src="../app-resources/android/splash/splash.9.png" />
        <splash density="land-ldpi" src="../app-resources/android/splash/splash.9.png" />
        <splash density="land-mdpi" src="../app-resources/android/splash/splash.9.png" />
        <splash density="land-xhdpi" src="../app-resources/android/splash/splash.9.png" />
        <splash density="port-hdpi" src="../app-resources/android/splash/splash.9.png" />
        <splash density="port-ldpi" src="../app-resources/android/splash/splash.9.png" />
        <splash density="port-mdpi" src="../app-resources/android/splash/splash.9.png" />
        <splash density="port-xhdpi" src="../app-resources/android/splash/splash.9.png" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <allow-navigation href="about:*" />
    <allow-navigation href="http://192.168.56.1:8080" />
</widget>
